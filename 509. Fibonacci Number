# 509. Fibonacci Number

# Link: https://leetcode.com/problems/fibonacci-number/description/

# Solution:
# Approach 1:

class Solution(object):
    def fib(self, n):
        """
        :type n: int
        :rtype: int
        """
        if n==0 or n==1:
            return n
        
        return self.fib(n-1)+self.fib(n-2)



# Approach 2:

class Solution(object):
    def fib(self, n):
        """
        :type n: int
        :rtype: int
        """
        if n<=1:
            return n
        
        a,b=0,1
        for _ in range(2,n+1):
            a,b=b,a+b
        return b

            
