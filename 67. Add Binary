# https://leetcode.com/problems/add-binary/?envType=problem-list-v2&envId=math


class Solution(object):
    def addBinary(self, a, b):
        """
        :type a: str
        :type b: str
        :rtype: str
        """
        x,y=int(a,2),int(b,2)
        ans=bin(x+y)
        a=ans[2:]
        return a
