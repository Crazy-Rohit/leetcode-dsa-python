class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        x=len(nums)
        list=[]
        for i in range(0,x):
            for j in range(i+1,x):
                if nums[i]+nums[j]==target:
                    list.append(i)
                    list.append(j)

        return list
        
